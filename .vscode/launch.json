    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python Debugger: FastAPI",
            "type": "debugpy",
            "request": "launch",
            //"program": "${file}",
            //"console": "integratedTerminal",
            "module": "poetry",
            "cwd": "${workspaceFolder}/src",
            "console": "integratedTerminal",
            "python": "/usr/local/bin/python",
            "args": ["run", "uvicorn", "main:app", "--reload"]

        }
        ,{
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            //"program": "${file}",
            //"console": "integratedTerminal",
            "module": "poetry",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "python": "/usr/local/bin/python",
            "args": ["run", "bpython", "-Xfrozen_modules=off", "${file}"]

        }
    ]
}